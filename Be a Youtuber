local function createSection(label, order)
    local lbl = Instance.new("TextLabel", Container)
    lbl.LayoutOrder         = order
    lbl.Size                = UDim2.new(1, 0, 0, 24)
    lbl.BackgroundTransparency = 1
    lbl.Text                = label
    lbl.TextColor3          = C.accent
    lbl.Font                = Enum.Font.GothamBold
    lbl.TextSize            = 10
    lbl.TextXAlignment      = Enum.TextXAlignment.Left
    lbl.ZIndex              = 4
end

-- Toggle button â€” returns (button, setVisual)
local function createToggle(label, order, callback)
    local baseName = label:gsub(": OFF", ""):gsub(": ON", "")

    local btn = Instance.new("TextButton", Container)
    btn.LayoutOrder         = order
    btn.Size                = UDim2.new(1, 0, 0, 36)
    btn.BackgroundColor3    = C.btn
    btn.Text                = baseName .. ": OFF"
    btn.TextColor3          = C.textMain
    btn.Font                = Enum.Font.GothamSemibold
    btn.TextSize            = 13
    btn.AutoButtonColor     = false
    btn.ZIndex              = 4
    Instance.new("UICorner", btn).CornerRadius = UDim.new(0, 10)

    -- Left accent strip (hidden when OFF)
    local strip = Instance.new("Frame", btn)
    strip.Size             = UDim2.new(0, 3, 0.6, 0)
    strip.Position         = UDim2.new(0, 0, 0.2, 0)
    strip.BackgroundColor3 = C.accent
    strip.BorderSizePixel  = 0
    strip.BackgroundTransparency = 1
    strip.ZIndex           = 5
    Instance.new("UICorner", strip).CornerRadius = UDim.new(0, 2)

    -- Status dot
    local dot = Instance.new("Frame", btn)
    dot.Size             = UDim2.new(0, 7, 0, 7)
    dot.Position         = UDim2.new(1, -16, 0.5, -3)
    dot.BackgroundColor3 = C.dot_off
    dot.BorderSizePixel  = 0
    dot.ZIndex           = 5
    Instance.new("UICorner", dot).CornerRadius = UDim.new(1, 0)

    local isOn = false

    local function setVisual(state)
        isOn = state
        btn.Text = baseName .. (state and ": ON" or ": OFF")
        TweenService:Create(btn,   TWEEN_SHORT, { BackgroundColor3 = state and C.btnOn or C.btn }):Play()
        TweenService:Create(dot,   TWEEN_SHORT, { BackgroundColor3 = state and C.dot_on or C.dot_off }):Play()
        TweenService:Create(strip, TWEEN_SHORT, { BackgroundTransparency = state and 0 or 1 }):Play()
    end

    btn.MouseEnter:Connect(function()
        TweenService:Create(btn, TWEEN_SHORT, {
            BackgroundColor3 = isOn and C.btnOnHover or C.btnHover
        }):Play()
    end)
    btn.MouseLeave:Connect(function()
        TweenService:Create(btn, TWEEN_SHORT, {
            BackgroundColor3 = isOn and C.btnOn or C.btn
        }):Play()
    end)
    btn.MouseButton1Click:Connect(function()
        setVisual(not isOn)
        callback(isOn)   -- passes new state
    end)

    return btn
end

-- One-shot action button
local function createAction(label, order, callback)
    local btn = Instance.new("TextButton", Container)
    btn.LayoutOrder         = order
    btn.Size                = UDim2.new(1, 0, 0, 36)
    btn.BackgroundColor3    = C.actionBtn
    btn.Text                = label
    btn.TextColor3          = C.textMain
    btn.Font                = Enum.Font.GothamSemibold
    btn.TextSize            = 13
    btn.AutoButtonColor     = false
    btn.ZIndex              = 4
    Instance.new("UICorner", btn).CornerRadius = UDim.new(0, 10)

    btn.MouseEnter:Connect(function()
        TweenService:Create(btn, TWEEN_SHORT, { BackgroundColor3 = C.actionHover }):Play()
    end)
    btn.MouseLeave:Connect(function()
        TweenService:Create(btn, TWEEN_SHORT, { BackgroundColor3 = C.actionBtn }):Play()
    end)
    btn.MouseButton1Click:Connect(function()
        TweenService:Create(btn, TweenInfo.new(0.08), { BackgroundColor3 = C.accentAlt }):Play()
        task.delay(0.12, function()
            TweenService:Create(btn, TWEEN_SHORT, { BackgroundColor3 = C.actionBtn }):Play()
        end)
        callback()
    end)
end

-- ============================================================
--  EGG ANIMATION REMOVAL (delayed)
-- ============================================================
task.spawn(function()
    task.wait(3)
    local animEvents = {"hatchExclusiveEgg","hatchEventEgg","hatchPet","showEgg","eggAnimation"}
    for _, name in ipairs(animEvents) do
        pcall(function()
            local ev = events:FindFirstChild(name)
            if ev and ev.OnClientEvent then
                for _, conn in pairs(getconnections(ev.OnClientEvent)) do
                    conn:Disconnect()
                end
            end
        end)
    end
end)

-- ============================================================
--  CHARACTER REFERENCE (kept up to date)
-- ============================================================
local char = player.Character or player.CharacterAdded:Wait()
local hrp  = char:WaitForChild("HumanoidRootPart")

player.CharacterAdded:Connect(function(c)
    char = c
    hrp  = c:WaitForChild("HumanoidRootPart")
end)

-- ============================================================
--  SECTION: AUTOMATION
-- ============================================================
createSection("âš¡   AUTOMATION", 0)

-- Floor positions
local floorPositions = {
    Vector3.new(-477.36, 11.40, -1931.17),
    Vector3.new(-475.44, 38.25, -1936.07),
    Vector3.new(-476.86, 65.60, -1931.54),
    Vector3.new(-473.64, 92.70, -1937.24),
    Vector3.new(-475.97, 118.71,-1931.32),
    Vector3.new(-476.07, 145.30,-1934.29),
}

createToggle("Auto Floor Cycle: OFF", 1, function(state)
    _G.AutoFloorCycle = state
    if state then
        task.spawn(function()
            while _G.AutoFloorCycle do
                for i = 1, #floorPositions do
                    if not _G.AutoFloorCycle then break end
                    pcall(function() hrp.CFrame = CFrame.new(floorPositions[i]) end)
                    task.wait(10)
                end
            end
        end)
    end
end)

createToggle("Auto Collect: OFF", 2, function(state)
    _G.AutoCollect = state
    if state then
        task.spawn(function()
            while _G.AutoCollect do
                pcall(function()
                    for _, p in pairs(workspace.Plots:GetChildren()) do
                        local builds = p:FindFirstChild("Builds")
                        if builds then
                            for _, item in pairs(builds:GetDescendants()) do
                                if not _G.AutoCollect then return end
                                if item:IsA("TouchTransmitter") then
                                    local part = item.Parent
                                    if part and part:IsA("BasePart") then
                                        firetouchinterest(player.Character.HumanoidRootPart, part, 0)
                                        task.wait(0.05)
                                        firetouchinterest(player.Character.HumanoidRootPart, part, 1)
                                    end
                                end
                            end
                        end
                    end
                end)
                task.wait(0.8)
            end
        end)
    end
end)

createToggle("Auto Upload: OFF", 3, function(state)
    _G.AutoUpload = state
    if state then
        task.spawn(function()
            while _G.AutoUpload do
                pcall(function() events.uploadAll:FireServer() end)
                task.wait(3)
            end
        end)
    end
end)

createToggle("Auto Claim: OFF", 4, function(state)
    _G.AutoClaim = state
    if state then
        task.spawn(function()
            while _G.AutoClaim do
                pcall(function() events.claimAll:FireServer() end)
                task.wait(3)
            end
        end)
    end
end)

