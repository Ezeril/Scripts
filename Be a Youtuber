-- ============================================================
--  SECTION: UPGRADES & REBIRTH
-- ============================================================
createSection("â¬†   UPGRADES & REBIRTH", 9)

local upgradeList = {"views","luck","viralChance","rebirthButtons","uploadTime"}

createToggle("Auto Upgrades: OFF", 10, function(state)
    _G.AutoUpgrades = state
    if state then
        task.spawn(function()
            while _G.AutoUpgrades do
                for _, upg in pairs(upgradeList) do
                    if not _G.AutoUpgrades then break end
                    pcall(function() events.buyUpgrade:FireServer(upg) end)
                    task.wait(1)
                end
                task.wait(5)
            end
        end)
    end
end)

createToggle("Smart Auto Rebirth: OFF", 11, function(state)
    _G.SmartAutoRebirth = state
    if state then
        task.spawn(function()
            while _G.SmartAutoRebirth do
                pcall(function()
                    local menu  = player.PlayerGui.ScreenGui.menus.rebirthMenu
                    local cont  = menu.container
                    local cash  = player.leaderstats.Cash.Value
                    local best, bestAmt = nil, 0
                    for _, b in pairs(cont:GetChildren()) do
                        if b:IsA("TextButton") then
                            local amt  = tonumber(b.rebirths.Text:match("%+(%d+)"))
                            local cost = tonumber(b.cost.Text:match("%$(%d+)"))
                            if amt and cost and cash >= cost and amt > bestAmt then
                                bestAmt = amt
                                best = b
                            end
                        end
                    end
                    if best then
                        events.rebirth:FireServer(bestAmt)
                    end
                end)
                task.wait(1)
            end
        end)
    end
end)

