    _G.SelectedYoutuber = Input.Text
end)
Input.Focused:Connect(function()
    TweenService:Create(inputStroke, TWEEN_SHORT, { Color = C.accent }):Play()
end)
Input.FocusLost:Connect(function()
    TweenService:Create(inputStroke, TWEEN_SHORT, { Color = C.separator }):Play()
end)

createToggle("Auto Buy: OFF", 21, function(state)
    _G.AutoBuy = state
    if state then
        task.spawn(function()
            while _G.AutoBuy do
                if _G.SelectedYoutuber ~= "" then
                    pcall(function() events.buyYoutuber:FireServer(_G.SelectedYoutuber) end)
                end
                task.wait(2)
            end
        end)
    end
end)

createAction("Sell All Youtubers", 22, function()
    pcall(function() events.sellAll:FireServer() end)
end)

-- ============================================================
--  SECTION: HATCHING
-- ============================================================
createSection("ðŸ¥š   HATCHING", 29)

local cratesLabel
pcall(function()
    cratesLabel = player:WaitForChild("PlayerGui")
        :WaitForChild("ScreenGui")
        :WaitForChild("menus")
        :WaitForChild("eventMenu")
        :WaitForChild("balance")
        :WaitForChild("crates")
end)

local openEventEgg = events:FindFirstChild("openEventEgg")

local function getCrates()
    if not cratesLabel then return 0 end
    return tonumber(cratesLabel.Text:match("%d+")) or 0
end

createToggle("Auto Bulk Hatch: OFF", 30, function(state)
    _G.AutoBulkHatch = state
    if state then
        task.spawn(function()
            while _G.AutoBulkHatch do
                pcall(function()
                    local n = getCrates()
                    if n > 0 and openEventEgg then
                        openEventEgg:FireServer(math.min(n, 10000))
                    end
                end)
                task.wait(1)
            end
        end)
    end
end)

-- ============================================================
--  MINIMIZE
-- ============================================================
local FULL_H = 448
local MINI_H = 57
local minimized = false

MinimizeBtn.MouseEnter:Connect(function()
    TweenService:Create(MinimizeBtn, TWEEN_SHORT, { BackgroundColor3 = C.btnHover }):Play()
end)
MinimizeBtn.MouseLeave:Connect(function()
    TweenService:Create(MinimizeBtn, TWEEN_SHORT, { BackgroundColor3 = C.btn }):Play()
end)

MinimizeBtn.MouseButton1Click:Connect(function()
    minimized = not minimized
    local target = minimized and UDim2.new(0, 282, 0, MINI_H) or UDim2.new(0, 282, 0, FULL_H)
    TweenService:Create(MainFrame, TWEEN_MED, { Size = target }):Play()
    TweenService:Create(GlowImg,   TWEEN_MED, { Size = minimized and UDim2.new(0, 340, 0, 110) or UDim2.new(0, 340, 0, 500) }):Play()
    Container.Visible = not minimized
    Footer.Visible    = not minimized
    Divider.Visible   = not minimized
    MinimizeBtn.Text  = minimized and "+" or "â€“"
end)
